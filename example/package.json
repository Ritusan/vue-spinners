{
  "name": "@saeris/vue-spinner-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve ./src/main.js",
    "build": "vue-cli-service build --modern --target app --name demo",
    "lint": "vue-cli-service lint",
    "test": "yarn test:unit",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "emotion": "9.2.6",
    "v-click-outside": "1.0.5",
    "vue": "^2.5.2",
    "vue-class-component": "6.2.0",
    "vue-color": "2.6.0",
    "vue-emotion": "0.4.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "eslint": "^5.4.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "lint-staged": "^7.2.2",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Saeris/vue-spinners"
  },
  "description": "Vue port of React Spinners",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "loading spinners",
    "loaders",
    "spinners",
    "loading indicators"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
